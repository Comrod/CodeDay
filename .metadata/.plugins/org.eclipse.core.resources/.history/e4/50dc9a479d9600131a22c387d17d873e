package com.Github.Comrod.Codeday.MagnusGravity;

import java.awt.image.BufferedImage;
import java.io.IOException;
import java.net.URL;

import javax.imageio.ImageIO;

public class Magnus
{

	private int dx;
	private int dy;
    private int x;
    private int y;
    
    public Magnus()
    {
    	x = 40;
    	y = 60;
    	BufferedImage magnus = null;{
    		try {
    			magnus = ImageIO.read(new URL("http://i.imgur.com/YXlnj1g.png"));
    			System.out.println("Gotten Background");
    		} catch (IOException e) {
    			System.out.println("Incorrect Background");
    		}}
    	
    }
    
    public void move()
    {
    	x += dx;
    	y += dy;
    	
    }
	
    public int getX()
    {
    	return x;
    }
    
    public int getY()
    {
    	return y;
    }
    
}
