package com.Github.Comrod.Codeday.MagnusGravity;

public class Magnus
{

	private int dx;
	private int dy;
    private int x;
    private int y;
    
    public Magnus()
    {
    	x = 40;
    	y = 60;
    }
    
    public void move()
    {
    	x += dx;
    	y += dy;
    	
    }
	
    public int getX()
    {
    	return x;
    }
    
    public int getY()
    {
    	return y;
    }
    
}
